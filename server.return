F.rand = function randomId(len, pattern) {
  if (!len) len = 30;
  if (!pattern) pattern = 'aA0';

  var chars = '';

  pattern.split('').forEach((a) => {
    if (!isNaN(parseInt(a))) {
      chars += possibilities.numbers;
    } else if (/[a-z]/.test(a)) {
      chars += possibilities.lowerCased;
    } else if (/[A-Z]/.test(a)) {
      chars += possibilities.capitals;
    } else {
      chars += possibilities.special;
    }
  });

  var result = '';

  for (var i = 0; i < len; i++) {
    result += chars.charAt(Math.floor(Math.random() * chars.length)); 
  }

  return result;
}
/return


/return
F.download = function(url, dest){
  var file = fs.createWriteStream(dest);
  return new Promise((resolve, reject) => {
    var responseSent = false;
    https.get(url, response => {
      response.pipe(file);
      file.on('finish', () =>{
        file.close(() => {
          if(responseSent)  return;
          responseSent = true;
          resolve();
        });
      });
    }).on('error', err => {
        if(responseSent)  return;
        responseSent = true;
        reject(err);
    });
  });
}
/return


